{
  "compilerOptions": {
    "checkJs": false,
    "moduleResolution": "node",
    "strict": true,
    "target": "es5",
    "lib": ["es2015", "dom"],
    "sourceMap": true,
    "noImplicitAny": true,
    "maxNodeModuleJsDepth": 2
  },
  "typeAcquisition": {
    "include": ["../types/**/*"],
    "enable": false
  }
}
